package org.bytearray.micrecorder.events{	import flash.events.Event;		public final class MicEvent extends Event	{		public static const ACTIVITY_LEVEL:String = "activityLevel";				private var _activityLevel:Number; // jordan added				/**		 * 		 * @param type		 * @param time		 * 		 */				public function MicEvent(type:String, activityLevel:Number = 0)		{			super(type, false, false);			_activityLevel = activityLevel		}		public function get activityLevel():Number		{			return _activityLevel;		}						public function set activityLevel(value:Number):void		{			_activityLevel = value;		}				/**		 * 		 * @return 		 * 		 */				public override function clone(): Event		{			return new MicEvent(type, _activityLevel)		}	}}